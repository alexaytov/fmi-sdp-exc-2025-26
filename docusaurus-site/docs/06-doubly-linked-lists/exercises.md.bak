// Move constructor
DoublyLinkedList(DoublyLinkedList&& other) noexcept
    : head(other.head), tail(other.tail) {
    // "–ö—Ä–∞–¥–µ–º" —Ä–µ—Å—É—Ä—Å–∏—Ç–µ –æ—Ç other
    other.head = nullptr;
    other.tail = nullptr;
}

// Move assignment
DoublyLinkedList& operator=(DoublyLinkedList&& other) noexcept {
    if (this != &other) {
        // –û—Å–≤–æ–±–æ–∂–¥–∞–≤–∞–º–µ —Å–æ–±—Å—Ç–≤–µ–Ω–∏—Ç–µ —Ä–µ—Å—É—Ä—Å–∏
        Node* current = head;
